{
  "manifest_version": 3,
  "name": "Athena",
  "version": "1.0.1",
  "description": "Painless UI navigation",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["styles.css"],
      "js": ["scripts/content.ts"]
    }
  ]
}